{
  "name": "mountain-utility",
  "version": "1.0.0",
  "description": "Simple shortcuts and tools to save time",
  "scripts": {
    "css:compile": "node-sass public/src/scss/main.scss public/src/scss/style.css",
    "publish": "npm run css:compile && cp node_modules/bootstrap/dist/js/bootstrap.bundle.js public/src/lib",
    "watch": "nodemon -e scss -x \"npm run css:compile\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mountant2021/odoo-utility.git"
  },
  "keywords": [
    "odoo",
    "viindoo",
    "runbot",
    "chrome",
    "browser",
    "utility",
    "extension"
  ],
  "author": "Droggol Infotech Private Limited, Mountain Coder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mountant2021/odoo-utility/issues"
  },
  "homepage": "https://github.com/Mountant2021/odoo-utility#readme",
  "devDependencies": {
    "bootstrap": "^5.3.0-alpha1",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20"
  }
}
